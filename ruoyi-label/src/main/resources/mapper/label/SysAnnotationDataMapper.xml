<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.label.mapper.SysAnnotationDataMapper">
    
    <resultMap type="SysAnnotationData" id="SysAnnotationDataResult">
        <result property="dataId"    column="data_id"    />
        <result property="taskId"    column="task_id"    />
        <result property="data"    column="data"    />
        <result property="version"    column="version"    />
        <result property="createTime"    column="create_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectSysAnnotationDataVo">
        select data_id, task_id, data, version, create_time, create_by, update_by, update_time, remark from sys_annotation_data
    </sql>

    <select id="selectSysAnnotationDataList" parameterType="SysAnnotationData" resultMap="SysAnnotationDataResult">
        <include refid="selectSysAnnotationDataVo"/>
        <where>  
            <if test="taskId != null  and taskId != ''"> and task_id = #{taskId}</if>
            <if test="data != null  and data != ''"> and data = #{data}</if>
            <if test="version != null  and version != ''"> and version = #{version}</if>
        </where>
    </select>
    
    <select id="selectSysAnnotationDataByDataId" parameterType="String" resultMap="SysAnnotationDataResult">
        <include refid="selectSysAnnotationDataVo"/>
        where data_id = #{dataId}
    </select>

    <insert id="insertSysAnnotationData" parameterType="SysAnnotationData">
        insert into sys_annotation_data
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dataId != null">data_id,</if>
            <if test="taskId != null and taskId != ''">task_id,</if>
            <if test="data != null and data != ''">data,</if>
            <if test="version != null and version != ''">version,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="updateBy != null and updateBy != ''">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dataId != null">#{dataId},</if>
            <if test="taskId != null and taskId != ''">#{taskId},</if>
            <if test="data != null and data != ''">#{data},</if>
            <if test="version != null and version != ''">#{version},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="updateBy != null and updateBy != ''">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateSysAnnotationData" parameterType="SysAnnotationData">
        update sys_annotation_data
        <trim prefix="SET" suffixOverrides=",">
            <if test="taskId != null and taskId != ''">task_id = #{taskId},</if>
            <if test="data != null and data != ''">data = #{data},</if>
            <if test="version != null and version != ''">version = #{version},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where data_id = #{dataId}
    </update>

    <delete id="deleteSysAnnotationDataByDataId" parameterType="String">
        delete from sys_annotation_data where data_id = #{dataId}
    </delete>

    <delete id="deleteSysAnnotationDataByDataIds" parameterType="String">
        delete from sys_annotation_data where data_id in 
        <foreach item="dataId" collection="array" open="(" separator="," close=")">
            #{dataId}
        </foreach>
    </delete>
</mapper>